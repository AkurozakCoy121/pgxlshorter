{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/:shortCode", "destination": "/api/redirect.js?short=:shortCode" }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
